import java.util.ArrayList;
import java.util.List;

public class ListExample {
    public static void main(String[] args) {
        
        List<String> myList = new ArrayList<>();

     
        myList.add("Java");
        myList.add("Python");
        myList.add("C++");

        
        for (String language : myList) {
            System.out.println(language);
        }
    }
}
