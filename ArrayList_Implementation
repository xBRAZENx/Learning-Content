import java.util.*;  
class ArrayListExample{  
    
 public static void main(String args[]){  
  
  ArrayList<String> al=new ArrayList<>();  
  al.add("Cat");  
  al.add("Dog");  
  al.add("Rat");  
  al.add("Bat");  
  
  Iterator<String> itr=al.iterator();  
  while(itr.hasNext()){  
   System.out.println(itr.next());  
  }  
  al.add(1,"Cow");
  System.out.println(al);
 }  
}  
